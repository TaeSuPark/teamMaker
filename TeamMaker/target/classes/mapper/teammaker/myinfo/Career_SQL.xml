<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.peachBro.teammaker.myinfo.dao.CareerDAO">
	<select id="careerList" parameterType="careerVO" resultType="careerVO">
		SELECT 
			* 
		FROM 
			CAREERS
		WHERE 
			USERID = #{userId}
	</select>
</mapper>